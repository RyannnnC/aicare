{
  "expo": {
    "name": "aicareProvider",
    "slug": "aicareProvider",
    "version": "1.0.3",
    "orientation": "default",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist":{
        "PhotoUsageDescription": "This app uses the photo to verify personal identity and complete personal information",
        "LocationUsageDescription": "This app uses the location to complete organisation information of location."
      },
      "supportsTablet": true,
      "bundleIdentifier": "aicare.provider"
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "app.aicare.provider"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
