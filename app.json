{
  "expo": {
    "name": "aicareProvider",
    "slug": "aicareProvider",
    "version": "1.3.5",
    "orientation": "default",
    "icon": "./assets/logo.png",
    "plugins":["@react-native-voice/voice"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist":{
        "NSPhotoLibraryUsageDescription": "This app uses the photo as avatar only, which would display on your personal information",
        "NSCameraUsageDescription": "This app uses the camera to take photo as avatar only, which would display on your personal information",
      },
      "supportsTablet": true,
      "bundleIdentifier": "aicare.provider"
    },
    "android": {
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "app.aicare.provider"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
